package fr.itris.glips.svgeditor.io.managers.dialog;

import java.io.*;
import javax.swing.filechooser.FileFilter;

/**
 * the abstract class of all the file filters
 * 
 * @author Jordi SUC
 */
public abstract class AbstractFileFilter extends FileFilter{

	/**
	 * whether the given file is accepted by this filter
	 * @param f a file
	 * @return true to accept the file
	 */
	public abstract boolean acceptFile(File f);
	
	/**
	 * @return the default extension
	 */
	public abstract String getDefaultExtension();
}
